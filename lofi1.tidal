setcps(0.5)
let chords =
      ("<d'min7 g'dom7 c'maj7 c'maj7>")
    roots = "<d g c c>"
  in do
  d1 $ up (fastGap 2 $ arpg chords)
    # sound "superpiano"
    # gain 0.7
    # room 1
    # legato 10
  d2 $ sound "[bd:3 [sn cymbols*3]]"
    # gain 1.2
    # room 0.4
    # pan (slow 4 $ sine)
  d3 $ echoWith 4 (1/8) (|* gain "0.8")
    $ stutWith 4 (1/8) (|* gain "0.8")
    $ sound "[cp:4]"
    # gain 0.8
    # room 1
  d4 $ every 4 (mask "~")
    $ n "2 2 2 2"
    # gain 0.3
    # s "cr:4"
  d5 $ up chords
    # sound "superhammond"
    # voice 6
    # lpf (range 200 1200 $ slow 0.3 sine)
  d6 $ struct "t*2"
