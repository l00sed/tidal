    $ up (arpg chords)
    # sound "bass:1"
  d7 $ s "birds*4"
    # n (irand 10)
    # pan rand
    # gain 0.5
    # room 0.5
  d8
    $ every 8 (mask "~ ~ ~ ~ ~ ~")
    $ s "jap"
    # n (irand 10)
    # pan rand
    # gain 1.1
    # room 0.5
  d10
    $ s "vinyl"
    # gain 0.9

hush

d1 $ amp 0
d1 $ amp 1
d2 $ amp 0
d2 $ amp 1
d3 $ amp 0.01
d3 $ amp 1
d4 $ amp 0
d4 $ amp 1
